<div>
	<main>
		<div class="card">
			<div class="card-body">
				{{#with heading}}


					<div class="card">
						<div class="card-body">
						{{#with main}}
							<h1>{{title}}</h1>
							<p>{{text}}</p>
							<div>
								{{#with button_text}}
								<button class="btn btn-primary">{{solid}}</button>
								<button class="btn btn-outline-primary">{{outline}}</button>
								{{/with}}
							</div>
						{{/with}}
						</div>
					</div>

					<br>

					<div class="card">
						<div class="card-body">
							{{#with second}}
								<h2>{{title}}</h2>
								<p>{{text}}</p>
								<div>
									{{#with button_text}}
									<button class="btn btn-secondary">{{solid}}</button>
									<button class="btn btn-outline-secondary">{{outline}}</button>
									{{/with}}
								</div>
							{{/with}}
						</div>
					</div>

					<br>

					<div class="card">
						<div class="card-body">
							{{#with subblock}}
								<h3>{{title}}</h3>
								<p>{{text}}</p>
								<div>
									{{#with button_text}}
									<button class="btn btn-dark">{{solid}}</button>
									<button class="btn btn-outline-dark">{{outline}}</button>
									{{/with}}
								</div>
							{{/with}}
						</div>
					</div>

				{{/with}}	
			</div>	
		</div>

		<br>
		<br>

		<div class="card">
			<div class="card-body">
			{{#with nested_display}}
				<div class="display-1">
					{{title}}
				</div>
				
				<br>

				<div class="card">
					<div class="card-body">

						{{#with children_display}}

							<div class="display-4">
								{{title}}
							</div>

							<p>{{text}}</p>

							<div>
								{{#with button}}
								<button class="btn btn-outline-warning">
									{{text}}
								</button>
								{{/with}}
							</div>

						{{/with}}
						
					</div>
				</div>

				<br>

				<div>
					<h6>
						{{header_text}}
					</h6>

					<div>
						{{#with button}}
							<button class="btn btn-info">
								{{text}}
							</button>
						{{/with}}
					</div>
				</div>
				
			{{/with}}
			</div>
		</div>

		<br>

		<div class="card">
			<div class="card-body">
				{{#with paragraph}}

				<p class="text-uppercase">
					{{title}}
				</p>

				<p class="text-lowercase">
					{{text}}
				</p>

				<br>

				<div>
					{{#with button}}
						<a href="{{url}}" class="btn btn-link" {{#if blank}} target="_blank"{{/if}} >{{text}}</a>
					{{/with}}
				</div>

				<br>

				<div class="card">
					<div class="card-body">
						<ul>
							{{#each list_name}}
								<li>
									<a href="{{url}}" class="btn btn-link" {{#if blank}} target="_blank"{{/if}} >{{text}}</a>
								</li>
							{{/each}}
						</ul>
					</div>
				</div>

				{{/with}}
			</div>	
		</div>	
	</main>
</div>